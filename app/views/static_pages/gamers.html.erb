<h1>Gamers in the House</h1>

<% @users = User.all %>
<% @users.each do |user| %>
<ul class="list">
  <%= image_tag user.avatar.url, :class=>'tinyAv' %>
  <%= user.gamertag %> 
  <span class='smlFont'><%= user.location  %></span>
</ul>
<% end %>









